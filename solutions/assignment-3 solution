def fibonacci_max_value(max_value):
    fib_sequence = []
    a, b = 0, 1
    while a <= max_value:
        fib_sequence.append(a)
        a, b = b, a + b  
    return fib_sequence
max_value = int(input("Enter the maximum value: "))
print(f"Fibonacci sequence up to {max_value} is:")
print(fibonacci_max_value(max_value))

