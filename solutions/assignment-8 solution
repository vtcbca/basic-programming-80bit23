def print_alphabet_pattern(n):
    i = 1  
    while i <= n:  
        print(" " * (n - i), end="")  
        j = 1
        while j <= i:
            print(chr(64 + j), end=" ")  
            j += 1  
        j = i - 1  
        while j >= 1:
            print(chr(64 + j), end=" ")
            j -= 1  
        print()
        i += 1
n = int(input("Enter the number of rows: "))
print_alphabet_pattern(n)

